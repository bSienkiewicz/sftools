{
  "manifest_version": 3,
  "name": "SF Tools",
  "version": "0.3.11",
  "action": {
    "default_popup": "index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{a6e378f1-8030-422a-81ec-b568dae16322}",
      "strict_min_version": "58.0"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["src/background.js"]
  },
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.force.com/*"
      ],
      "js": [
        "src/TemplateContent.jsx",
        "src/SevenDays.jsx"
      ],
      "run_at": "document_idle"
    }
  ]
}